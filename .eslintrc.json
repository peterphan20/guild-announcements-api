{
  "extends": ["airbnb-base", "plugin:node/recommended"],
  "rules": {
    "consistent-return": "off",
    "global-require": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "semi": ["error", "never"]
  }
}
